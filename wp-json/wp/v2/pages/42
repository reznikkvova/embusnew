s.blur();\n            else this.focus();\n            return false;\n        });\n        \n        function selectCar() {\n            if (this.isSameNode(selectedCar)) return;\n            selectedCar.classList.remove('selected');\n            this.classList.add('selected');\n            this.appendChild(selectedCarSvg);\n            selectedCar = this;\n            carCurrent.replaceChild(this.children[0].cloneNode(true), carCurrent.children[0]);\n        }\n        \n        for (var i=0; i<carOptionsList.length; i++) {\n            carOptionsList[i].addEventListener('mousedown', selectCar);\n        }\n        \n        car.addEventListener('keydown', function(e) {\n            if (e.keyCode == '13') { \/\/ Enter\n                e.preventDefault();\n                workers.focus();\n                return false;\n            } else if (e.keyCode == '38') { \/\/ up\n                e.preventDefault();\n                var prev = selectedCar.previousElementSibling;\n                if (!prev) return;\n                selectCar.call(prev);\n                return false;\n            } else if (e.keyCode == '40') { \/\/ down\n                e.preventDefault();\n                var next = selectedCar.nextElementSibling;\n                if (!next) return;\n                selectCar.call(next);\n                return false;\n            }\n        });\n        \n        \/\/ Workers\n        var workers = document.getElementById('hmOrderWorkers');\n        var workersMinus = document.getElementById('hmOrderWorkersMinus')\n        var workersPlus = document.getElementById('hmOrderWorkersPlus')\n        var workersMin = 0;\n        var workersMax = 3;\n        \n        function getWorkersCount() {\n            return parseInt(workers.value) || 0;\n        }\n        \n        function workersButtonsUpdate() {\n            var count = getWorkersCount();\n            if (count <= workersMin) {\n                workersMinus.classList.add('hm-order-disabled');\n            } else {\n                workersMinus.classList.remove('hm-order-disabled');\n            }\n            if (count >= workersMax) {\n                workersPlus.classList.add('hm-order-disabled');\n            } else {\n                workersPlus.classList.remove('hm-order-disabled');\n            }\n        }\n        \n        workers.addEventListener('input', workersButtonsUpdate);\n        \n        workersMinus.addEventListener('click', function() {\n            var count = getWorkersCount() - 1;\n            if (count < workersMin) {\n                workers.value = workersMin;\n            } else if (count > workersMax) {\n                workers.value = workersMax\n            } else {\n                workers.value = count;\n            }\n            workersButtonsUpdate();\n        })\n        \n        workersPlus.addEventListener('click', function() {\n            var count = getWorkersCount() + 1;\n            if (count < workersMin) {\n                workers.value = workersMin;\n            } else if (count > workersMax) {\n                workers.value = workersMax\n            } else {\n                workers.value = count;\n            }\n            workersButtonsUpdate();\n        })\n        \n        \/\/ Contacts\n        var name = document.getElementById('hmOrderName');\n        var phone = document.getElementById('hmOrderPhone');\n        var message = document.getElementById('hmOrderMsg')\n        \n        name.addEventListener('input', function() {\n            name.classList.remove('hm-order-invalid');\n        })\n        \n        phone.addEventListener('input', function() {\n            phone.classList.remove('hm-order-invalid');\n        })\n        \n        \/\/ Clear\n        function clearAll() {\n            \/\/ locations\n            locationFrom.value = '';\n            locationTo.value = '';\n            locationFrom.classList.remove('hm-order-invalid');\n            \n            \/\/ car selector\n            selectCar.call(carOptionsList[1]);\n            \n            \/\/ workers\n            workers.value = '1';\n            workersButtonsUpdate();\n            \n            \/\/ contacts\n            name.value = '';\n            phone.value = '';\n            message.value = '';\n            name.classList.remove('hm-order-invalid');\n            phone.classList.remove('hm-order-invalid');\n        }\n        \n        \/\/ Challenge\n        var challengeWidget = '#challengeContainer';\n        var challengeToken = null;\n        window.onloadTurnstileCallback = function () {\n            turnstile.render(challengeWidget, {\n                sitekey: '0x4AAAAAAALGwef6AjmVICMu',\n                callback: function(token) {\n                    challengeToken = token;\n                },\n            });\n        };\n        \n        \/\/ Submit\n        var submit = document.getElementById('hmOrderSubmit');\n        var overlay = document.getElementById('hmOrderOverlay');\n        var overlayLoading = document.getElementById('hmOrderOverlayLoading');\n        var overlaySuccess = document.getElementById('hmOrderOverlaySuccess');\n        var overlaySuccessBtn = document.getElementById('hmOrderOverlaySuccessBtn');\n        var overlayFail = document.getElementById('hmOrderOverlayFail');\n        var overlayFailBtn = document.getElementById('hmOrderOverlayFailBtn');\n        var formData;\n        \n        function orderData() {\n            this.action = 'hm_order';\n            this.address_from = '';\n            this.address_to = '';\n            this.car = '';\n            this.workers = '';\n            this.name = '';\n            this.phone = '';\n            this.message = '';\n            this.challengeToken = challengeToken;\n            return this;\n        }\n        \n        function send() {\n            $.ajax({\n                type: 'POST',\n                dataType: 'json',\n                url: '../wp-admin/admin-ajax.php',\n                data: formData,\n                success: function (data, textStatus, XMLHttpRequest) {\n                    if (data.status == 'success') {\n                        clearAll();\n                        overlayLoading.classList.add('hidden');\n                        overlaySuccess.classList.remove('hidden');\n                        return;   \n                    }\n                    overlayLoading.classList.add('hidden');\n                    overlayFail.classList.remove('hidden');\n                },\n                error: function() {\n                    overlayLoading.classList.add('hidden');\n                    overlayFail.classList.remove('hidden');\n                }\n            });\n        }\n\n        submit.addEventListener('click', function() {\n            if (!challengeToken) return;\n            formData = new orderData();\n            formData.car = selectedCar.dataset.car;\n            formData.workers = workers.value;\n            var error = false;\n            if (!(formData.address_from = locationFrom.value)) {\n                locationFrom.classList.add('hm-order-invalid');\n                error = true;\n            } else locationFrom.classList.remove('hm-order-invalid');\n            formData.address_to = locationTo.value\n            if (!(formData.name = name.value)) {\n                name.classList.add('hm-order-invalid');\n                error = true;\n            } else name.classList.remove('hm-order-invalid');\n            if (!(formData.phone = phone.value)) {\n                phone.classList.add('hm-order-invalid');\n                error = true;\n            } else phone.classList.remove('hm-order-invalid');\n            if (error) return;\n            formData.message = message.value\n            formData.challengeToken = challengeToken;\n            overlayLoading.classList.remove('hidden');\n            overlay.classList.remove('hidden');\n            send();\n            challengeToken = null;\n            turnstile.reset(challengeWidget);\n        });\n        \n        overlaySuccessBtn.addEventListener('click', function() {\n            overlay.classList.add('hidden');\n            overlaySuccess.classList.add('hidden');\n        });\n        \n        overlayFailBtn.addEventListener('click', function() {\n            overlay.classList.add('hidden');\n            overlayFail.classList.add('hidden');\n        });\n        \n    })(jQuery);\n    });\n<\/script>\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-3147e65 elementor-hidden-mobile elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"3147e65\" data-element_type=\"section\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-no\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-a116d4f\" data-id=\"a116d4f\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-61ad0be elementor-widget elementor-widget-image\" data-id=\"61ad0be\" data-element_type=\"widget\" data-widget_type=\"image.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<img decoding=\"async\" width=\"130\" height=\"155\" src=\"../wp-content/uploads/2022/10/decor-after-cover.svg\" class=\"attachment-full size-full wp-image-6479\" alt=\"\" \/>\t\t\t\t\t\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-ad694cb hm-section-slice elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"ad694cb\" data-element_type=\"section\" data-settings=\